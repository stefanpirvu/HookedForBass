```java
package com.example.demo.model;

import jakarta.persistence.*;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.NonNull;

@Entity
@Data
@NoArgsConstructor
@AllArgsConstructor
public class SeniueloDuro {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long idSeniueloDuro;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_seniuelo", nullable = false)
    private Seniuelo idSeniuelo;

    @NonNull
    @Column(nullable = false)
    private String tipo;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_profundidad", nullable = false)
    private ProfundidadBuceo idProfundidad;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_clase_buceo", nullable = false)
    private ClaseBuceo idClaseBuceo;
}
```