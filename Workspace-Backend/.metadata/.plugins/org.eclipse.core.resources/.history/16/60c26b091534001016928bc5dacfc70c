package com.example.demo.model;

import jakarta.persistence.*;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.NonNull;

@Entity
@Data
@NoArgsConstructor
@AllArgsConstructor
public class Glidebait {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long idGlidebait;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_seniuelo", nullable = false)
    private Seniuelo idSeniuelo;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_profundidad_buceo", nullable = false)
    private ProfundidadBuceo idProfundidadBuceo;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_clase_buceo")
    private ClaseBuceo idClaseBuceo;
}
