package com.example.demo.model;

import jakarta.persistence.*;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;
import lombok.NonNull;

@Entity
@Data
@NoArgsConstructor
@AllArgsConstructor
public class SeniueloDuro {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long idSeniueloDuro;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_seniuelo")
    private Seniuelo idSeniuelo;

    @NonNull
    private String tipo;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_profundidad")
    private ProfundidadBuceo idProfundidad;

    @NonNull
    @ManyToOne
    @JoinColumn(name = "id_clase_buceo")
    private ClaseBuceo idClaseBuceo;
}
